{
  "name": "admin",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:studio": "pnpm --filter database studio"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "cross-env": "^7.0.3",
    "prettier": "latest",
    "shared": "workspace:^1.0.0",
    "turbo": "latest",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.1",
  "eslintIgnore": [
    "dist/**/*",
    "node_modules"
  ]
}
